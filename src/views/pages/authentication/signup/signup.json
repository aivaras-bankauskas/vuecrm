{
    "config": {
        "name": "signup",
        "API": "/register",
        "redirect": "/team"
    },
    "formData": {
        "firstName": "",
        "lastName": "",
        "email": "",
        "password": "",
        "confirmPassword": ""
    },
    "inputs": [
        {
            "inputName": "firstName",
            "inputType": "text",
            "rules": "required|alpha|min:2"
        },
        {
            "inputName": "lastName",
            "inputType": "text",
            "rules": "required|alpha"
        },
        {
            "inputName": "email",
            "inputType": "email",
            "rules": "required|email"
        },
        {
            "inputName": "password",
            "inputType": "password",
            "rules": "required|password|min:6"
        },
        {
            "inputName": "confirmPassword",
            "inputType": "password",
            "rules": "required|password|confirmPassword|min:6"
        }
    ]
}