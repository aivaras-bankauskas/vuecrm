{
    "config": {
        "name": "signIn",
        "API": "/users",
        "redirect": "/"
    },
    "formData": {
        "email": "",
        "password": ""
    },
    "inputs": [
        {
            "inputName": "email",
            "inputType": "email",
            "rules": "required|email"
        },
        {
            "inputName": "password",
            "inputType": "password",
            "rules": "required|password|mismatch|min:6"
        }
    ]
}